@Unpublished{RPD15,
  Title                    = {A Side-Channel Attack Against the Secret Permutation on an Embedded {McEliece} Cryptosystem},
  Author                   = {Richmond, Tania and Petrvalsk\'{y}, Martin and Milo\v{s} Drutarovsk\'{y}},
  Note                     = {TRUDEVICE 2015},
  Year                     = {2015},

  Abstract                 = {In this paper, based on a thorough analysis of the state of the art, we point out a missing solution for embedded devices to secure the syndrome computation. We show that this weakness can open the door to a side-channel attack targeting the secret permutation. Indeed, brute-force attack iterations are dramatically decreased when the secret permutation is recovered. We demonstrate the feasibility of this attack against the McEliece cryptosystem implemented on an ARM Cortex-M3 microprocessor using Goppa codes. We explain how to recover the secret permutation on a toy example. Finally, we propose a promising countermeasure, which can be implemented in embedded devices to prevent this attack.},
  Owner                    = {Tania Richmond},
  Timestamp                = {2015.03.25}
}

@InProceedings{MR20,
  author   = {Marquer, Yoann and Richmond, Tania},
  booktitle= {2020 IEEE 27th Symposium on Computer Arithmetic (ARITH)}, 
  title    = {{A Hole in the Ladder : Interleaved Variables in Iterative Conditional Branching}}, 
  year     = {2020},
  volume={},
  number={},
  pages={56-63},
  doi={10.1109/ARITH48897.2020.00017}
  abstract = {The modular exponentiation is crucial to the RSA cryptographic protocol, and variants inspired by the Montgomery ladder have been studied to provide more secure algorithms.
In this paper, we abstract away the iterative conditional branching used in the Montgomery ladder, and formalize systems of equations necessary to obtain what we call the semi-interleaved and fully-interleaved ladder properties.
In particular, we propose two attacker models using fault-injection able to obtain bits of the secret in the semi-interleaved cases (including the Montgomery ladder) but none in the fully-interleaved cases.
We also apply these equations in order to extend the Montgomery ladder for both the semi- and fully-interleaved cases, thus proposing novel and more secure algorithms to compute the modular exponentiation.}
}

