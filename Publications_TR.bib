  Author                   = {Arora, Shiraj and Legay, Axel and Richmond, Tania and Traonouez, Louis-Marie},
  Booktitle                = {Leveraging Applications of Formal Methods, Verification and Validation. Verification (ISoLA)},
  Year                     = {2018},

  Address                  = {Cham},
